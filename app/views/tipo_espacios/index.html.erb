<p id="notice"><%= notice %></p>

<h1>Tipo Espacios</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_espacios.each do |tipo_espacio| %>
      <tr>
        <td><%= tipo_espacio["idTipoEspacio"] %></td>
        <td><%= tipo_espacio["nombre"] %></td>
        <td><%= link_to 'Destroy', tipo_espacio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Espacio', new_tipo_espacio_path %>
