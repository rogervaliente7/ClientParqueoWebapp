<p id="notice"><%= notice %></p>

<h1>Tipo Reservas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_reservas.each do |tipo_reserva| %>
      <tr>
        <td><%= tipo_reserva["descripcion"] %></td>
        <td><%= tipo_reserva["idTipoReserva"] %></td>
        <td><%= tipo_reserva["nombre"] %></td>
        <td><%= tipo_reserva["publico"] %></td>
        <td><%= link_to 'Destroy', tipo_reserva, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Reserva', new_tipo_reserva_path %>
